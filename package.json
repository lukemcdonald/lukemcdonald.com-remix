{
  "name": "luke-mcdonald-remix",
  "private": true,
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/tailwind.css -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "remix setup node",
    "predeploy": "npm run build",
    "deploy:remote": "fly deploy --remote-only",
    "deploy:local": "fly deploy --local-only",
    "deploy": "fly --app lukemcdonald deploy .",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "cloudinary": "^1.29.0",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "front-matter": "^4.0.2",
    "marked": "^4.0.14",
    "postcss-import": "^14.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remix": "^1.4.0",
    "tiny-invariant": "^1.2.0",
    "use-local-storage-state": "^17.0.1"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@types/eslint": "^8.4.1",
    "@types/marked": "^4.0.3",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
