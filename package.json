{
  "name": "luke-mcdonald-remix",
  "private": true,
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/tailwind.css -o ./src/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles/tailwind.css -o ./src/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "deploy": "fly deploy --remote-only",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "cloudinary": "^1.28.1",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.1.1",
    "front-matter": "^4.0.2",
    "marked": "^4.0.12",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@remix-run/dev": "^1.2.3",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.12",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.7",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
