{
  "name": "luke-mcdonald-remix",
  "private": true,
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/tailwind.css -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles/tailwind.css -o ./app/styles/tailwind.css --watch",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "predeploy": "npm run build",
    "deploy:remote": "fly deploy --remote-only",
    "deploy:local": "fly deploy --local-only",
    "deploy": "fly --app lukemcdonald deploy .",
    "start": "remix-serve build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^2.0.11",
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@tailwindcss/typography": "^0.5.4",
    "cloudinary": "^1.30.1",
    "cloudinary-build-url": "^0.2.4",
    "clsx": "^1.2.1",
    "front-matter": "^4.0.2",
    "marked": "^4.0.18",
    "postcss-import": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "use-local-storage-state": "^18.1.0"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/eslint": "^8.4.5",
    "@types/marked": "^4.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
